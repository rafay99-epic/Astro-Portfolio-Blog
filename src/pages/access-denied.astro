---
import BaseHead from "@components/AstroComponent/base/BaseHead.astro";
import ComingSoon from "@components/ReactComponent/error/ComingSoon";
import authorConfig from "@config/siteConfig/info.json";

const featureName = Astro.url.searchParams.get("feature") ?? "Exciting Feature";
export const prerender = true;
---

<!doctype html>
<html lang="en">
  <head>
    <BaseHead
      title={`Access Denied | ${authorConfig.SiteName}`}
      description={`Sorry, you don't have access to this ${featureName}.`}
    />
  </head>
  <body>
    <main class="pt-15 mt-0">
      <ComingSoon featureName={featureName} client:load />
    </main>
  </body>
</html>
