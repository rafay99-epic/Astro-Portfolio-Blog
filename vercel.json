{
    "version": 2,
    "build": {
        "command": "npm run vercel-build",
        "cache": {
            "paths": [
                "node_modules/.playwright"
            ]
        }
    },
    "routes": [
        {
            "src": "/snaprescue.sh",
            "dest": "/snaprescue.sh"
        },
        {
            "src": "/Meaning-Mate-APK",
            "dest": "/Meaning-Mate-APK.apk"
        }
    ]
}