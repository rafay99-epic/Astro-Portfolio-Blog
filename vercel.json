{
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm install && npx playwright install-deps && npx playwright install chromium",
    "env": {
        "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD": "false"
    },
    "routes": [
        {
            "src": "/snaprescue.sh",
            "dest": "/snaprescue.sh"
        },
        {
            "src": "/Meaning-Mate-APK",
            "dest": "/Meaning-Mate-APK.apk"
        }
    ]
}